{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "yiisoft/yii2-swiftmailer": "^2.1",
        "yiisoft/yii2-jui": "^2.0",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "yii2mod/yii2-rbac": "^2.3",
        "symfony/polyfill-php72": "^1.23",
        "select2/select2": "^4.0",
        "schmunk42/yii2-giiant": "@stable",
        "kartik-v/yii2-widget-select2": "@dev",
        "lajax/yii2-translate-manager": "^1.8",
        "omgdef/yii2-multilingual-behavior": "~2.0",
        "kartik-v/yii2-detail-view": "dev-master",
        "yiisoft/mailer-symfony": "^1.0",
        "symfony/google-mailer": "^5.4",
        "symfony/twig-bridge": "^5.4",
        "bizley/migration": "^4.3",
        "kartik-v/yii2-money": "*",
        "kartik-v/yii2-dynagrid": "dev-master",
        "kartik-v/yii2-export": "dev-master",
        "yii2tech/html2pdf": "^1.0",
        "kartik-v/yii2-mpdf": "dev-master",
        "kartik-v/yii2-password": "@dev",
        "yiisoft/yii2-symfonymailer": "*",
        "kartik-v/yii2-bootstrap4-dropdown": "@dev",
        "ellera/yii2-backup": "^2.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.2",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "codeception/module-phpbrowser": "^2.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
